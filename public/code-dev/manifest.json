{
  "short_name": "Code - DEV",
  "name": "Code",
  "icons": [
    {
      "src": "/code-dev/images/code-192-teal.png",
      "type": "image/png",
      "sizes": "192x192"
    },
    {
      "src": "/code-dev/images/code-512-teal.png",
      "type": "image/png",
      "sizes": "512x512"
    }

  ],
  "start_url": "/code-dev/index.html",
  "display": "standalone",
  "display_override": ["window-control-overlay", "standalone"],
  "scope": "/code-dev/",
  "theme_color": "#00aabb",
  "background_color": "#ffffff",
  "shortcuts": [
    {
      "name": "Open last project",
      "short_name": "last",
      "description": "Open Code with the last active project file",
      "url": "/code-dev/index.html?open=last",
      "icons": [{ "src": "/code-dev/images/code-192-teal.png", "sizes": "192x192" }]
    }
  ],
  "description": "Super light code editing in your brower",
  "screenshots": [
    {
      "src": "images/screen-small-720x540.png",
      "type": "image/png",
      "sizes": "720x540"
    },
    {
      "src": "images/screen-mid-1024x662.png",
      "type": "image/png",
      "sizes": "1024x662"
    }
  ],
  "file_handlers": [
    {
      "action": "/code-dev/openFile.html",
      "accept": {
        "application/javascript": [".js",".mjs",".jsm",".json"],
        "text/stylesheet": [".css"],
        "text/html": [".html"]
      },
      "icons": [
        {
          "src": "/code-dev/images/code-192-teal.png",
          "sizes": "192x192",
          "type": "image/png"
        }
      ]
    }
  ]
}